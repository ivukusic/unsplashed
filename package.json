{
  "name": "unsplash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "prettier": "prettier --write ./src/**/*.{ts,tsx}",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint_check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --silent",
    "test_coverage": "jest --coverage --silent",
    "test_watch": "jest --watch"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.17",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-navigation/native": "^6.0.7",
    "@react-navigation/stack": "^6.1.0",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.25.0",
    "axios-mock-adapter": "^1.20.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "react": "17.0.2",
    "react-native": "0.67.1",
    "react-native-flipper": "^0.131.1",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-svg": "^12.1.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-vector-icons": "^9.0.0",
    "react-query": "^3.34.12",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-flipper": "^2.0.1",
    "redux-persist": "^6.0.0",
    "styled": "^1.0.0",
    "styled-components": "5.1.0",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/runtime": "^7.16.7",
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.2",
    "@types/react-native": "^0.66.9",
    "@types/styled-components-react-native": "^5.1.0",
    "@types/styled-system": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^27.4.6",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "jest": "^27.5.0",
    "jest-styled-components": "7.0.2",
    "lint-staged": "^10.5.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^27.1.3",
    "typescript": "^3.8.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./jest.setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./node_modules/react-native/jest/setup.js",
      "./node_modules/react-native/Libraries/Utilities/__mocks__/BackHandler.js"
    ],
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|@react-native-community|@react-navigation)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "resolutions": {
    "styled-components": "^5"
  }
}